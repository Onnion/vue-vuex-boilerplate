{
  "compilerOptions": {
    "lib": [
      "dom",
      "es5",
      "es2015",
      "es2015.promise"
    ],
    "baseUrl": "./",
    "paths": {
      "./node_modules/@types/" : ["custom-types/jasmine/index.d.ts"]
    },
    "module": "es2015",
    "moduleResolution": "node",
    "target": "es5",
    "sourceMap": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "exclude": [
      "typings/browser.d.ts",
      "typings/browser",
      "node_modules"
      "node_modules"
    ]
  }
}
